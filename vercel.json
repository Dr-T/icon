{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/generate-logo",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/$1"
    }
  ]
}